set number
set nocompatible
set tabstop=4 softtabstop=0 expandtab shiftwidth=2 smarttab
set backspace=indent,eol,start
set history=50
set ruler
set showcmd
set incsearch
set hlsearch
set background=dark

let g:netrw_liststyle = 3
let g:netrw_banner = 0

syntax on

" let g:solarized_termtrans=1
" colorscheme solarized

if has('mouse')
  set mouse=a
endif

